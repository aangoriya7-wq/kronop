{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/components/*": [
        "./components/*"
      ],
      "@/app/*": [
        "./app/*"
      ],
      "@/context/*": [
        "./context/*"
      ],
      "@/constants/*": [
        "./constants/*"
      ]
    },
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "declaration": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "target": "esnext",
    "lib": [
      "dom",
      "esnext"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "types/**/*.d.ts",
    "services/**/*.d.ts"
, "app/Player/song.js"  ]
}
